#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main() {
	int a[5] = {5,1,3,2,4};
	int d;
	int e = 0;
	for(int b = 0;b<4;b++){
		for(int c = 0;c<4-b;c++){
			if(a[c]>a[c+1]){
				d = a[c];
				a[c]=a[c+1];
				a[c+1]= d;
			}
 	    }
	}
	while(e < 5){
		printf("%d",a[e]);
		e++;
	}
	return 0;
}
